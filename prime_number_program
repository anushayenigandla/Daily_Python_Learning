#prime number
def is_prime(num):
    if num < 2:
        return False
    if num in range(2,num+1):
        if num % 2 == 0:
            return False
        else:
            return True
print(is_prime(8))

#given a number print the prime numbers below that number

n=20
for i in range(2,n):
    for j in range(2,i//2+1):
        if i % j == 0:
            break
    else:
        print(i)
